{
    "name": "dft/silverstripe-deferedimages",
    "description": "Deferres loading of template images",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/Dean-Forest-Tech/silverstripe-deferedimages",
    "keywords": ["silverstripe", "images"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dean Forest Tech",
            "email": "info@dean-forest-tech.uk"
        }
    ],
    "support": {
        "issues": "http://github.com/Dean-Forest-Tech/silverstripe-deferedimages/issues"
    },
    "require": {
        "silverstripe/framework": "^4 || ^5",
        "silverstripe/assets": "^1 || ^2",
        "dft/silverstripe-deferedimages-theme": "^1",
        "heyday/silverstripe-responsive-images": "^3"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "DFT\\SilverStripe\\DeferedImages\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
